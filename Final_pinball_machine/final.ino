// Load Wi-Fi library
#include <WiFi.h>

// Load Servo lib & object
#include <ESP32Servo.h>   
Servo leftServo, rightServo;

// LCD
#include <LiquidCrystal_I2C.h> 
#include <Wire.h> 
#define I2C_SDA 13
#define I2C_SCL 14

LiquidCrystal_I2C lcd(0x27, 16, 2); // set the LCD address to 0x27 for a 16 chars and 2 line display

// Set Pin
int relay = 7;
int ray0 = 41;
int ray1 = 0;
int ray2 = 48;
int ray3 = 19;

// variables
bool flag = false;
int points = 0;

// Replace with your network credentials

const char* ssid     = "312552024"; //REPLACE_WITH_YOUR_SSID
const char* password = "123456789";  //REPLACE_WITH_YOUR_PASSWORD

// Set web server port number to 80
WiFiServer server(80);

// Variable to store the HTTP request
String header;

// Auxiliar variables to store the current output state (左、右伺服馬達狀態、彈珠發射狀態)
String output45State = "off";
String output48State = "off";
String output0State = "off";



// Set your Static IP address 
IPAddress local_IP(192, 168, 4, 1); 

// Set your Gateway IP address 
IPAddress gateway(192, 168, 50, 1); 

IPAddress subnet(255, 255, 255, 0); 
//IPAddress primaryDNS(8, 8, 8, 8); //optional 
//PAddress secondaryDNS(8, 8, 4, 4); //optional

unsigned long lastTriggerTime = 0;
unsigned long triggerDelay = 20000; // reset after 20s
int relayState = -1;


void IRAM_ATTR get_point() {
  flag = true;
}
 

void setup() {
  Serial.begin(115200);


  // Connect to Wi-Fi network with SSID and password
  Serial.print("Setting AP (Access Point)…");
  // Remove the password parameter, if you want the AP (Access Point) to be open
  WiFi.softAPConfig(local_IP, local_IP, subnet);
  WiFi.softAP(ssid, password);

  IPAddress IP = WiFi.softAPIP();
  Serial.print("AP IP address: ");
  Serial.println(IP);
  
  server.begin();

  leftServo.attach(9);  // 設定要將伺服馬達接到哪一個PIN腳
  rightServo.attach(10);

  pinMode(relay,OUTPUT);
  pinMode(ray0, INPUT);
  pinMode(ray1, INPUT);
  pinMode(ray2, INPUT);
  pinMode(ray3, INPUT);

  attachInterrupt(ray0, get_point, FALLING);
  attachInterrupt(ray1, get_point, FALLING);
  attachInterrupt(ray2, get_point, FALLING);
  attachInterrupt(ray3, get_point, FALLING);

  digitalWrite(relay, HIGH);

  Wire.begin(I2C_SDA, I2C_SCL);
  lcd.init(); // initialize the lcd 
  lcd.backlight(); // turn on backlight. 

}

void loop(){
  // for points & show points
   if(flag){
    points++;
    flag = false;
  }

  lcd.clear(); // Clears the LCD screen
  lcd.setCursor(0,0);
  lcd.print("Scores: ");
  lcd.print(points);
  delay(100);


  // reset relay to high after 20s
  relayState = digitalRead(relay);
  if(digitalRead(relay) == LOW && millis() - lastTriggerTime > triggerDelay)
  {
    digitalWrite(relay, HIGH);
    Serial.print("reset relay to ");
    relayState = digitalRead(relay);
    Serial.println(relayState);
  }
  //Serial.println(points);

  WiFiClient client = server.available();   // Listen for incoming clients

  if (client) {                             // If a new client connects,
    Serial.println("New Client.");          // print a message out in the serial port
    String currentLine = "";                // make a String to hold incoming data from the client
    while (client.connected()) {            // loop while the client's connected
      if (client.available()) {             // if there's bytes to read from the client,
        char c = client.read();             // read a byte, then
        Serial.write(c);                    // print it out the serial monitor
        header += c;

        if (c == '\n') {                    // if the byte is a newline character
          // if the current line is blank, you got two newline characters in a row.
          // that's the end of the client HTTP request, so send a response:
          if (currentLine.length() == 0) {
            // HTTP headers always start with a response code (e.g. HTTP/1.1 200 OK)
            // and a content-type so the client knows what's coming, then a blank line:
            client.println("HTTP/1.1 200 OK");
            client.println("Content-type:text/html");
            client.println("Connection: close");
            client.println();
            
            // turns the buttons on and off
            if (header.indexOf("GET /45/on") >= 0) {
              Serial.println("Left Flipper on");
              output45State = "on";
              leftServo.write(90);  //旋轉到0度，就是一般所說的歸零
              delay(200);
              leftServo.write(0); //旋轉到90度
            } 
            else if (header.indexOf("GET /45/off") >= 0) {
              Serial.println("Left Flipper off");
              output45State = "off";
            } 
            else if (header.indexOf("GET /48/on") >= 0) {
              Serial.println("Right Flipper on");
              output48State = "on";
              rightServo.write(90);  //旋轉到0度，就是一般所說的歸零
              delay(200);
              rightServo.write(180); //旋轉到90度
            } 
            else if (header.indexOf("GET /48/off") >= 0) {
              Serial.println("Right Flipper off");
              output48State = "off";
            } 
            else if (header.indexOf("GET /0/on") >= 0) {
              Serial.println("Start on");
              output0State = "on";
              digitalWrite(relay, LOW);
              lastTriggerTime = millis();
            } 
            else if (header.indexOf("GET /0/off") >= 0) {
              Serial.println("Start off");
              output0State = "off";
            } 
            
            // Display the HTML web page
            client.println("<!DOCTYPE html><html>");
            client.println("<head><meta name=\"viewport\" content=\"width=device-width, initial-scale=1\">");
            client.println("<meta charset=\"utf-8\">");
            client.println("<link rel=\"icon\" href=\"data:,\">");
            // CSS to style the on/off buttons 
            // Feel free to change the background-color and font-size attributes to fit your preferences
            client.println("<style>html { font-family: Helvetica; display: inline-block; margin: 0px auto; text-align: center;}");
            client.println(".button { background-color: #4CAF50; border: none; color: white; padding: 16px 40px;");
            client.println("text-decoration: none; font-size: 20px; margin: 2px; cursor: pointer;}");
            client.println(".button2 {background-color: #555555;}</style></head>");
            
            // Web Page Heading
            client.println("<body><h1>AIoT Project: 彈珠台</h1>");
            
            // 左檔板按鈕與判斷條件 
            client.println("<a href=\"/45/on\"><button class=\"button\">Left</button></a>"); 
            
            // 右檔板按鈕與判斷條件      
            client.println("<a href=\"/48/on\"><button class=\"button\">Right</button></a>");

            client.println("<p></p>");
            // 開始鈕與判斷條件     
            client.println("<a href=\"/0/on\"><button class=\"button\">Start</button></a>");

            client.println("<p></p>");
            // 印出三個按鈕的狀態
            client.println("<p>Status:</br>");
            client.println("Left button: ");
            client.println(output45State);
            client.println("</br>Right button: ");
            client.println(output48State);
            client.println("</br>Start button: ");
            client.println(output0State);
            client.println("</p>");

          
            client.println("<p></p>");
            // 網頁下方圖案
            // Ref: https://randomnerdtutorials.com/display-images-esp32-esp8266-web-server/
            client.println("<img src=\"data:image/jpeg;base64,\"  width=\"300\" height=\"300\">" );
            client.println("</body></html>");
            
            // The HTTP response ends with another blank line
            client.println();
            // Break out of the while loop
            break;
          } 
          else { // if you got a newline, then clear currentLine
            currentLine = "";
          }
        } 
        else if (c != '\r') {  // if you got anything else but a carriage return character,
          currentLine += c;      // add it to the end of the currentLine
        }
      }
    }
    // Clear the header variable
    header = "";
    // Close the connection
    client.stop();
    Serial.println("Client disconnected.");
    Serial.println("");
  }
}